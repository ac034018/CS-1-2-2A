print("A 2D array is just a list where each element is also a list")
arr = [
  ["aa","bb","cc"],
  ["dd","ee","ff"],
  ["jj","hh","ii"]
]

print("\nPrint the 0th element, often referred to as a  row")
print("arr[0][0]:",arr[0][0])

print("\nPrint the 0th 'column' of the 0th 'row'")
print("arr[0][0]:",arr[0][0])

print("\nPrint arr in 2D")
for row in arr:
  print(row)

print("\nPrint each element of the array individually")
for row in arr:
  print(row)
for col in row:
  print(col)
